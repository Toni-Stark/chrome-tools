{
  "name": "url溯源工具",
  "version": "5",
  "manifest_version": 2,
  "description": "火狐便捷工具",
  "icons": {
    "19": "assets/images/icon_19.png",
    "38": "assets/images/icon_38.png",
    "128": "assets/images/icon_128.png"
  },
  "applications": {
    "gecko": {
      "id": "extension@example.com"
    }
  },
  "background": {
    "persistent": true,
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "./content.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "debugger",
    "tabs",
    "webRequest",
    "activeTab",
    "storage",
    "cookies",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*",
    "<all_urls>"
  ],
  "commands": {
    "speak": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Start speaking"
    },
    "download": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "Download selection"
    }
  },
  "browser_action": {
    "default_title": "Wavenet for Chrome",
    "default_popup": "popup/popup.html"
  },
  "homepage_url": "https://github.com/wavenet-for-chrome/extension"
}
